<%= form_with model: [:admin, @test, @question], local: true do |form| %>
  <%= render 'shared/errors', resource: @question %>

  <p>
    <%=form.label :body %>
    <%=form.text_field :body, :size=>"50", placeholder: "Make an offer ", :maxlength=>"1254"%>
  </p>

  <p><%= form.submit %></p>
<% end %>
