<tr>
  <td><%= test.title %></td>
  <td><%=test_level(test)%></td>
  <td> <%= test.category.name %></td>
  <td> <%= test.questions.count %></td>
  <td><%= link_to t('shared.action.show'), admin_test_path(test) %></td>
  <td><%= link_to t('shared.action.edit'), edit_admin_test_path(test) %></td>

  <td> <%= link_to t('shared.action.delete'),
                   admin_test_path(test),
                   method: :delete,
                   data: { confirm: t('shared.action.confirm')} %> </td>
</tr>
